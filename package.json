{
  "name": "bs-revamp",
  "version": "0.1.0",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "test": "npm run build 6& jest",
    "test:watch": "jest --watchAll",
    "bench": "npm run build && node lib/js/__bench__/bench.js"
  },
  "homepage": "https://github.com/glennsl/bs-revamp#readme",
  "bugs": "https://github.com/glennsl/bs-revamp/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glennsl/bs-revamp.git"
  },
  "keywords": [
    "reason",
    "bucklescript",
    "regex",
    "regular expressions"
  ],
  "license": "MIT",
  "author": "glennsl",
  "devDependencies": {
    "bs-platform": "1.8.3",
    "bs-jest": "^0.1.0",
    "bs-loader": "^1.5.12",
    "bs-benchmarkjs": "glennsl/bs-benchmarkjs"
  },
  "jest": {
    "moduleFileExtensions": [
      "re",
      "js",
      "ml"
    ],
    "testMatch": [
      "**/__tests__/*_test.re"
    ],
    "transform": {
      ".(re|ml)": "bs-loader"
    }
  }
}
